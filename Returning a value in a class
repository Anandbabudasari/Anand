//volume() returns the volume of a box
public class Box {
	double width;
	double height;
	double depth;
	
	//compute and return volume 
	
	double  volume() {
		
		return width*height*depth;
		
	}
}

public class boxDemo4 {

	public static void main(String[] args) {
		Box mybox1=new Box();
		Box mybox2=new Box();
		double vol;
		
		//asign values to mybox1's instance variables 
		
		mybox1.width=19;
		mybox1.height=29;
		mybox1.depth=33;
		
		//assigning  values to mybox2
		
		mybox2.width=45;
		mybox2.height=12;
		mybox2.depth=33;
		
		//get volume of first box
		
		vol=mybox1.volume();
		System.out.println("Volume is"+vol);
		
		//get volume of second box
		vol=mybox2.volume();
		System.out.println("volume is"+vol);
		
		

	}

}
