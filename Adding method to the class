//This program include a method inside the box class 
public class Box {
	double width;
	double height;
	double depth;
	
	//display volume of a box
	void volume() {
		System.out.println("Volume is ");
		System.out.println(width*height*depth);
		
	}
}
public class BoxDemo_3 {

	public static void main(String[] args) {
		Box mybox1= new Box();
		Box mybox2=new Box();
		
		//assign values to mybox's instance variables 
		mybox1.width=10;
		mybox1.height=10;
		mybox1.depth=16;
		
		//assign different values to mybox'2 instance variables 
		
		mybox2.width =13;
		mybox2.height=24;
		mybox2.depth=38;
		
		//display volume of first box 
		mybox1.volume();
		
		//display volume of second box
		mybox2.volume();
		

	}

}
