//this program has two Box objects 
public class Box {
	double width;
	double height;
	double depth;
}
//this class declares an object of type box 
public class BoxDemo {

	public static void main(String[] args) {
		Box mybox =new Box();
		double vol;
		
		//assiging values to  mybox instance variables
		mybox.width=10;
		mybox.height=30;
		mybox.depth=16;
		
		//compute volume of box
		vol=mybox.width*mybox.height*mybox.depth;
		
		System.out.println("volume is " +vol);
				
		
	

	}

}
public class BoxDemo2 {

	public static void main(String[] args) {
	Box mybox1 =new Box();
	Box mybox2=new Box();
	double vol;
	
	//assign different values to mubox1's instance variables 
	mybox1.width=10;
	mybox1.height=30;
	mybox1.depth=16;
	
	//assign different values to mybox2
	mybox2.width=19;
	mybox2.height=20;
	
	mybox2.depth=34;
	
	//compute volume of first box
	 
	vol = mybox1.width*mybox1.height*mybox1.depth;
	System.out.println("Volume is "+vol);
	
	
	//finding volume of another box
	
	vol= mybox2.width*mybox2.height*mybox2.depth;
	System.out.println("Volume of second box is " +vol);
	

	}

}
